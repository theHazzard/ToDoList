extends layout

append head
	script(src="http://code.jquery.com/jquery-1.9.1.min.js")
	script(src="/javascripts/underscore-min.js", type="text/javascript")
	script(src="/javascripts/backbone.js", type="text/javascript")		
block content
	div#wrapper
		h1 Sus tareas
		p
			if todos
				each todo in todos
					h5 #{todo.mensaje}
	script(src="/javascripts/custom.js", type="text/javascript")